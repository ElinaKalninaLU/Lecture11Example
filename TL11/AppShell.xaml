<?xml version="1.0" encoding="UTF-8" ?>
<Shell
    x:Class="TL11.AppShell"
    xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
    xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
    xmlns:local="clr-namespace:TL11"
    Shell.FlyoutBehavior="Flyout"
    Title="TL11">

    <ShellContent
        Title="Course"
        ContentTemplate="{DataTemplate local:Courses}"
        Route="Courses" />

    <ShellContent
        Title="Student"
        ContentTemplate="{DataTemplate local:StudentsView}"
        Route="StudentsView" />
    <ShellContent
            Title="Person"
    ContentTemplate="{DataTemplate local:PersonEntryPage}"
    Route="PersonEntryPage" />
    <ShellContent
           Title="StudentsCourse"
   ContentTemplate="{DataTemplate local:StudentCourse}"
   Route="StudentCourse" />

</Shell>
